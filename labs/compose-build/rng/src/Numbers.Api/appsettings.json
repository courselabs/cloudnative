{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Rng" : {
    "Range": {
      "Min": 0,
      "Max": 100
    },
    "FailAfter": {
      "Enabled": false,
      "CallCount" : 3,
      "Action" : "Exit"
    }
  }
}